        "textPosition": "CENTER",
        "textAlignment": "CENTER"
    }
},


    // ******** MOBILE ******** //

    "responsive-layout.mobile#treinamento": { // Linhas da página (1ª Título, 2ª Subtítulo e 3ª Imagem Regions)
        "children": [
            "flex-layout.row#treinamento1Mobile",
            "flex-layout.row#treinamento2Mobile",
            "flex-layout.row#treinamento3Mobile"
        ]
    },
    "flex-layout.row#treinamento1Mobile": {}, // Linha do Título - Regions - Mobile
    "flex-layout.row#treinamento2Mobile": {},
